ErrorDocument 404 /error404.html

Options +FollowSymLinks
<IfModule mod_rewrite.c>

    RewriteEngine On

#	RewriteCond %{HTTP_HOST} ^(www.?)lamosca.info$
#   RewriteRule    ^$ /info/ [L]
#	RewriteCond %{HTTP_HOST} ^(www.?)lamosca.info$
#   RewriteRule    (.*) /info/$1 [L]

	RewriteCond %{HTTP_HOST} ^www.lamosca.info$
    RewriteRule    ^$ /info/ [L]
	RewriteCond %{HTTP_HOST} ^www.lamosca.info$
    RewriteRule    (.*) /info/$1 [L]

	RewriteCond %{HTTP_HOST} ^lamosca.info$
    RewriteRule    ^$ /info/ [L]
	RewriteCond %{HTTP_HOST} ^lamosca.info$
    RewriteRule    (.*) /info/$1 [L]

	RewriteCond %{HTTP_HOST} ^info.lamosca.com$
    RewriteRule    ^$ /info/ [L]
	RewriteCond %{HTTP_HOST} ^info.lamosca.com$
    RewriteRule    (.*) /info/$1 [L]

#	RewriteCond %{HTTP_HOST} ^bcnmcr.lamosca.com$
#   RewriteRule    ^$ /bcnmcr/index.html [L]
	RewriteCond %{HTTP_HOST} ^bcnmcr.lamosca.com$
	RewriteRule    ^$ /bcnmcr/ [L]
	RewriteCond %{HTTP_HOST} ^bcnmcr.lamosca.com$
	RewriteRule    (.*) /bcnmcr/$1 [L]

	RewriteCond %{HTTP_HOST} ^(www\.)?festadelaciencia.cat$
  RewriteRule    ^$ /clients/festadelaciencia/ [L]
	RewriteCond %{HTTP_HOST} ^(www\.)?festadelaciencia.cat$
  RewriteRule    (.*) /clients/festadelaciencia/$1 [L]

  RewriteCond %{HTTP_HOST} ^(www\.)?elbarriadria.com$
  RewriteRule    ^$ /clients/elbarri/ [L]
  RewriteCond %{HTTP_HOST} ^(www\.)?elbarriadria.com$
  RewriteRule    (.*) /clients/elbarri/$1 [L]

	RewriteCond %{HTTP_HOST} ^festadelaciencia.lamosca.com$
    RewriteRule    ^$ /clients/festadelaciencia/ [L]
	RewriteCond %{HTTP_HOST} ^festadelaciencia.lamosca.com$
    RewriteRule    (.*) /clients/festadelaciencia/$1 [L]

	RewriteCond %{HTTP_HOST} ^(.*?)winter.cat$
    RewriteRule    ^$ /amics/thomas/ [L]
	RewriteCond %{HTTP_HOST} ^(.*?)winter.cat$
    RewriteRule    (.*) /amics/thomas/$1 [L]

#   RewriteCond %{HTTP_HOST} ^(www\.)?domain2\.com$ [NC]
