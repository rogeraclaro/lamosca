#	RewriteRule ^([0123456789]+)/?$ index.php?cid=$1 [L]
#	RewriteRule ^([0123456789]+)/([0123456789]+)/?$ index.php?cid=$1&pid=$2 [L]
#	RewriteCond %{REQUEST_fileNAME} !-f


# 	rss feed
	RewriteCond %{REQUEST_fileNAME} !-f
	RewriteRule ^rss.xml$ /ext/rss.php [NC,L,QSA]

	RewriteRule ^tshirt/$ /tshirt/coleccio.htm [L]
	RewriteRule ^(mosaic)/?$ /mosaic/index.php [L]
	RewriteRule ^mosaic/index.html$ /mosaic/index.php [L]

	RewriteRule ^admin/$ /admin/index.php [L]
	RewriteRule ^admin$ /admin/index.php [L]


	RewriteCond %{REQUEST_fileNAME} !-f
	RewriteRule ^([a-zA-Z0-9_.-]+)/$ /index.php?curl=$1 [L]

	RewriteCond %{REQUEST_fileNAME} !-f
	RewriteRule ^([a-zA-Z0-9_.-]+)/([a-z0-9_.-]+).html$ /index.php?curl=$1&purl=$2 [L]

</IfModule>
