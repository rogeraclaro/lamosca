/* General: */

@font-face {
    font-family: 'lmsc';
    src: url('../tipo/lmsc-webfont.eot');
    src: url('../tipo/lmsc-webfont.eot?#iefix') format('embedded-opentype'), url('../tipo/lmsc-webfont.woff2') format('woff2'), url('../tipo/lmsc-webfont.woff') format('woff'), url('../tipo/lmsc-webfont.ttf') format('truetype'), url('../tipo/lmsc-webfont.svg#lmscregular') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'lmsc2';
    src: url('../tipo/lmsc2-webfont.eot');
    src: url('../tipo/lmsc2-webfont.eot?#iefix') format('embedded-opentype'), url('../tipo/lmsc2-webfont.woff2') format('woff2'), url('../tipo/lmsc2-webfont.woff') format('woff'), url('../tipo/lmsc2-webfont.ttf') format('truetype'), url('../tipo/lmsc2-webfont.svg#lmscregular') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'lmsc3';
    src: url('../tipo/lmsc3-webfont.eot');
    src: url('../tipo/lmsc3-webfont.eot?#iefix') format('embedded-opentype'), url('../tipo/lmsc3-webfont.woff2') format('woff2'), url('../tipo/lmsc3-webfont.woff') format('woff'), url('../tipo/lmsc3-webfont.ttf') format('truetype'), url('../tipo/lmsc3-webfont.svg#lmscregular') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'lmsc4';
    src: url('../tipo/lmsc4-webfont.eot');
    src: url('../tipo/lmsc4-webfont.eot?#iefix') format('embedded-opentype'), url('../tipo/lmsc4-webfont.woff2') format('woff2'), url('../tipo/lmsc4-webfont.woff') format('woff'), url('../tipo/lmsc4-webfont.ttf') format('truetype'), url('../tipo/lmsc4-webfont.svg#lmscregular') format('svg');
    font-weight: normal;
    font-style: normal;
}

html {
    font-family: Helvetica, "Arial Narrow", Verdana, Arial, sans-serif;
    font-size: 10px;
    font-weight: bold;
    color: #888;
    margin: 0;
    padding: 0;
    background-color: #fff;
}

body {
    text-align: left;
    background: url(pics/bg.gif) no-repeat;
    margin: 0;
    padding: 0;
    overflow-x: hidden;
}

p {
    line-height: 120%;
    padding: 0;
    margin: 0;
}


/* Layout */

#container {
    margin: 0;
    padding: 0;
    /* width: 1085px; */
    max-width: 100%;
}

#project {
    clear: both;
    margin: 0;
    padding: 0;
    position: relative;
    top: 50px;
    max-width: 100%;
}

#projectinfo {
    float: left;
    margin: 0;
    padding: 0;
    width: 140px;
    position: absolute;
    top: 0px;
    left: 25px;
    z-index: 2;
}

#project #modules {
    margin: 0;
    position: absolute;
    top: 0px;
    max-width: 100%;
    min-width: 925px;
    z-index: 1;
}

#project #wrap {
    padding: 0 10px 0 325px;
    max-width: 100%
}

#project img {
    margin: 0;
    padding: 0 0 10px 0;
    display: block;
    width: 100%;
}


/* start cat information */

#catModule {
    width: 760px;
    margin: 10px 0 10px 0;
}

.catTitle {
    float: left;
    width: 290px;
    margin: 0 10px 30px 0;
}

.indexTitleText {
    font-size: 9px;
    margin: 0;
    padding: 0 0 2px 0;
}

.catTitleText {
    font-size: 9px;
    margin: 0;
    padding: 0 0 6px;
    background: url(pics/line_cat.gif) no-repeat bottom left;
}

.catTitleDrop {
    margin: 0;
    padding: 0;
}

select.js_index {
    font-family: Verdana, Arial, Helvetica;
    font-weight: normal;
    font-size: 9px;
    width: 140px;
}


/* content div information */

.pageTitle {
    color: #333;
    font-size: 16px;
    font-weight: bold;
}

.pageInfo {
    font-size: 9px;
    width: 140px;
    margin: 0 10px 230px 0;
    float: left;
}

.pageFacebook {
    clear: both;
    margin: 10px 0 0 0;
}

.pageFacebook img {
    padding: 0 0 2px 0 !important;
}

.pageSponsor {
    clear: both;
    margin: 110px 0 0 0;
}

.textModule {
    margin: 0 0 10px 0;
    display: block;
}

.flashModule {
    margin: 0 0 7px 0;
    display: block;
}

.break {
    clear: both;
}


/* Navigation */

#navigation {
    height: 18px;
    margin: 0;
    padding: 39px 0 0 25px;
    max-width: 100%;
}

form {
    margin: 0;
    padding: 0;
}

#htmlnav {
    float: left;
    font-size: 10px;
}

#htmlnav {
    float: left;
    font-size: 10px;
}

#htmlnav ul {
    list-style: none;
    padding: 0;
    margin: 0;
}

#htmlnav ul.subnavi {
    list-style: none;
    padding: 0 0 0 10px;
    margin: 0;
}

#js_nav_1 {
    float: left;
    width: 140px;
    margin: 0;
    padding: 0;
}

#js_nav_2 {
    float: left;
    width: 140px;
    padding: 0;
    margin: 0 0 0 10px;
}

select.js_nav {
    font-family: Verdana, Arial, Helvetica;
    font-weight: normal;
    font-size: 9px;
    width: 125px;
}


/* Links texto */

a:link,
a:visited {
    text-decoration: none;
    color: #999999;
}

a:active,
a:hover {
    color: #000000;
}

h1 {
    background: transparent url(pics/logo.gif) no-repeat top left;
    width: 263px;
    height: 110px;
    float: left;
}

h1 span {
    display: none;
}

h2 {
    font-family: Georgia, "Times New Roman", Times, serif;
    font-style: italic;
    font-size: 1.6em;
    color: #d99f9a;
    margin-top: 4px;
}

h3 {
    color: #656868;
    margin: 10px 0 7px 0;
    font-size: 1.2em;
    line-height: 140%;
}

h4 {
    font-size: 1em;
    margin: 20px 0 1px 0;
}

h5 {
    font-size: 1em;
    margin: 20px 0 -8px 0;
}

.lamosca {
    font-family: lmsc2;
    font-size: 20px;
    color: #3b3b3b;
    letter-spacing: -0.6px;
}

.adresa {
    font-family: lmsc4;
    font-size: 9px;
    color: #8f8f8f;
    text-transform: uppercase;
    margin-top: 80px;
    font-weight: lighter;
}

.adresa_home {
    font-family: lmsc4;
    font-size: 9px;
    color: #8f8f8f;
    text-transform: uppercase;
    margin-top: 80px;
    font-weight: lighter;
}

.adresa_mobile {
    font-family: lmsc4;
    font-size: 9px;
    color: #8f8f8f;
    text-transform: uppercase;
    margin-top: 80px;
    font-weight: lighter;
}

.adresa_mobile {
    display: none;
}

.feis {
    font-family: lmsc4;
    font-size: 9px;
    color: #8f8f8f;
    text-transform: uppercase;
    margin-top: 65px;
    line-height: 0px;
    font-weight: lighter;
}

@media only screen and (min-device-width: 320px) and (max-device-width: 680px) {
    body {
        background: url(pics/bg_mobile.gif) no-repeat;
    }

    #navigation {
        height: 50px;
        background-color: black;
    }

    #project #wrap {
        padding: 0 10px 0 25px;
        max-width: 100%;
        margin-top: 45px;
    }

    #projectinfo {
        /*
        background-color: white;
        top: 25px;
        opacity: .6;
        width: 92%;
    */
    }

    .indexTitleText {
        float: right;
        font-size: 30px;
        padding: 0px 0 10px 0;
        display: none;
    }

    form {
        margin: 0;
        padding: 0;
        position: absolute;
        /*right: 40px;*/
        top: 22px;
    }

    #js_nav_1 {
        width: 400px;
        margin: 0;
        padding: 0;
    }

    select.js_nav {
        font-family: lmsc4;
        text-transform: uppercase;
        font-size: 32px;
        width: 400px;
        color: black;
        background-color: white;
        margin-right: 30px;
    }

    select.js_nav option {
        background-color: white;
    }

    .adresa,
    .feis {
        font-size: 30px;
    }

    /*
    .adresa br {
    display: none;
    }

    br {
        display: -moz-inline-stack;
        display: inline-block;
        vertical-align: top;
        zoom: 1;
        *display: inline;
    }
    br {
        content: ' · ';
        width: 9px;
        height: 18px;
    }
    br:before {
        content: ' · '
    }
*/
    .feis,
    .pageSponsor {
        display: none;
    }

    .adresa_mobile {
        display: block;
        position: fixed;
        bottom: 0px;
        background-color: white;
        padding: 20px 25px 20px 25px;
        left: 0px;
        width: 92%;
        font-size: 30px;
    }

    .adresa_home {
        display: none;
    }

    .adresa {
        display: none;
    }

    .lamosca {
        font-size: 50px;
        top: -20px;
        position: relative;
    }

    #project img {
        padding: 0 0 25px 0;
    }

    table {
        width: 100%;
        font-size: 1em;
    }
}